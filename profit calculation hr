import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner s=new Scanner(System.in);
        int x,a,b;
        x=s.nextInt();
        a=s.nextInt();
        b=s.nextInt();
        int r=(x*a)-(100+(x*b));
        System.out.println(r);
    }
}
