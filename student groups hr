import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int p=s.nextInt();
        if(p>=100 && p<=200)
        {
            int r=p/4;
             System.out.println(r);
             System.out.println(r);
             System.out.println(r);
             System.out.println(r+p%4);   
        }
        else
             System.out.println("INVALID INPUT");
    }
}
